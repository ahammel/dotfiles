	name = Alex Hammel
	email = ahammel87@gmail.com
[core]
	autocrlf = false
	safecrlf = true
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[credential]
	helper = cache --timeout=3600
